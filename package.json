{
  "name": "invoice",
  "description": "Electron invoice app",
  "author": "Yalgie",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
